# Configuration d'environnement pour EcosystIA
# Copiez ce fichier en .env.local et configurez vos variables

# API Backend
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_BACKEND_URL=http://localhost:8000

# Clés API IA
VITE_GEMINI_API_KEY=votre_cle_gemini_api_key
VITE_OPENAI_API_KEY=votre_cle_openai_api_key

# Configuration développement
VITE_ENABLE_DEVTOOLS=true
VITE_DEBUG_MODE=true

# WebSocket
VITE_WS_URL=ws://localhost:8000/ws/

# Upload de fichiers
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,pdf,doc,docx,xls,xlsx
